{
    "name": "neercsys/flarum-ext-bosanski",
    "description": "Bosnian language pack for Flarum's third party extensions.",
    "type": "flarum-extension",
    "keywords": ["extension", "flarum", "bosanski", "locale", "translation"],
    "license": "MIT",
    "authors": [
        {
            "name": "neercsys",
            "email": "neercsys@gmail.com",
            "homepage": "https://www.balkanbuzz.net"
	    "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/neercsys/flarum-ext-bosanski/issues",
        "source": "https://github.com/neercsys/flarum-ext-bosanski"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.15-dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.9.x-dev"
        },
        "flarum-extension": {
            "title": "Bosanski",
            "icon": {
                "name": "fas fa-language",
		"background": "#346eeb",
		"backgroundSize": "cover",
		"backgroundPosition": "center",
		"color": "#e9f01d"
            }
        },
        "flarum-locale": {
            "code": "bs",
            "title": "Bosanski"
        }
    }
}
